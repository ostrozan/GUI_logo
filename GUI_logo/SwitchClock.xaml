<UserControl x:Class="GUI_logo.SwitchClock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GUI_logo"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Name="mainGroupBox" Header="Spínací hodiny" Padding="0,6,0,0">
        <Grid>
            <TextBlock Margin="33,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">On</TextBlock>
            <TextBlock Margin="0,0,60,0" VerticalAlignment="Top" HorizontalAlignment="Right">Off</TextBlock>
            <StackPanel Name="stpMain" Orientation="Vertical" HorizontalAlignment="Center" ButtonBase.Click="stpMain_Click" Margin="0,20,0,0" VerticalAlignment="Top"/>
        </Grid>
        

    </GroupBox>
</UserControl>
